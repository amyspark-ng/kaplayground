    "@astrojs/tailwind": "^5.1.0",
    "@monaco-editor/react": "^4.6.0",
    "@nanostores/persistent": "^0.10.1",
    "@prisma/client": "5.15.1",
    "@supabase/supabase-js": "^2.43.5",
    "astro": "^4.9.3",
    "daisyui": "^4.12.2",
    "flex-splitter-directive": "^0.5.1",
    "nanostores": "^0.10.3",
    "pako": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
